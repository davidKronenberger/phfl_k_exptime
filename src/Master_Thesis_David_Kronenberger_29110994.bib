@inproceedings{schewe2006fixpoint,
    abstract = {We add inflationary and non-inflationary fixed-points to higher-order logics. We show that, for every order, it is sufficient to increase the order of the given logic by one to capture inflationary fixed-points and by two to capture non-inflationary fixed-points. In the two cases, restricting to the existential fragment of the corresponding logic turns out to be enough. This also holds for non-deterministic fixed-points.},
    acmid = {1565045},
    added-at = {2018-05-25T14:43:55.000+0200},
